{
   "parserOptions": {
      "ecmaVersion": 6
   },
   "env": {
      "node": true
   },
   "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:prettier/recommended"
   ],
   "plugins": [
      "prettier"
   ],
   "rules": {
      "prettier/prettier": "error",
      "no-console": [
         "error",
         {
            "allow": [
               "warn",
               "error"
            ]
         }
      ]
   }
}